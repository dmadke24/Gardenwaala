    <script>
        function ShowNewsMsg(data) {
            if (data.status == true) {
                $('#ajaxResult').html('<div class="alert alert-success alert-dismissible fade in"><a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>           <strong>Success! </strong>' + data.msg + '</div>');
                $('#Email').val('');
                alert("Thank you!");
            }
            else {
                $('#ajaxResult').html('<div class="alert alert-danger alert-dismissible fade in"><a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>           <strong>Fail! </strong>' + data.msg + '</div>');
            }
        }

    </script>
